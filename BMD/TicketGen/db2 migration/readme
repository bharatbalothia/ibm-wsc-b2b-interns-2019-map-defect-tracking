 ------------------------------steps for migration of code from couchdb to db2 ---------------------------------
-->change the databasename,userid,password,portno of db2 in mirgate.js

-->create a schema named report

-->create the following tables in db2 with the below commands:

1)db2 create table report.accountmerge(PACCOUNT VARCHAR(200) NOT NULL, ACCOUNT VARCHAR(200) NOT NULL,REGION VARCHAR(10) NOT NULL, PRIMARY KEY(PACCOUNT))



2)db2 create table report.transactions(CID VARCHAR(200) NOT NULL, ACTIONBY VARCHAR(200) NOT NULL,ACTIONTIME VARCHAR(100) NOT NULL,ACTION VARCHAR(200) NOT NULL,DESCRIPTION VARCHAR(200), PRIMARY KEY(CID))

3)db2 create table report.ticketform(CID VARCHAR(200) NOT NULL, APPRECIATED_USERID VARCHAR(200) NOT NULL, APPRECIATED_USERNAME VARCHAR(200) NOT NULL, REASON VARCHAR(200), CLIENT VARCHAR(100) NOT NULL, TDATE DATE NOT NULL,FUNCTIONAL_TEAM VARCHAR(200) ,TICKET_CATEGORY VARCHAR(200) NOT NULL,RATINGTYPE VARCHAR(200) NOT NULL,SEVERITY VARCHAR(10) NOT NULL,STATE VARCHAR(200),STATUS VARCHAR(10) NOT NULL,TICKETNO VARCHAR(200) NOT NULL, TYPE VARCHAR(10) NOT NULL, USERID VARCHAR(50) NOT NULL,USERNAME VARCHAR(50) NOT NULL,APPRECIATION_TEXT VARCHAR(1000),FEEDBACK VARCHAR(100), PRIMARY KEY(CID))



4)db2 create table report.forbiddenusers(USERID VARCHAR(100) NOT NULL, PRIMARY KEY(USERID))

5)db2 create table report.nonticketform( ID VARCHAR(100) NOT NULL, APPRECIATED_USERID VARCHAR(100) NOT NULL,APPRECIATED_USERNAME VARCHAR(100) NOT NULL,APPRTXT VARCHAR(1000) NOT NULL, CLIENT VARCHAR(100) NOT NULL, TDATE DATE NOT NULL, FUNCTIONAL_TEAM VARCHAR(100) NOT NULL,APPRECIATED_BY VARCHAR(100) NOT NULL ,REASON VARCHAR(100), STATUS VARCHAR(10) NOT NULL, TYPE VARCHAR(100) NOT NULL, USERID VARCHAR(100) NOT NULL,USERNAME VARCHAR(100) NOT NULL , PRIMARY KEY(ID))


6)db2 create table report.escalationform( ID VARCHAR(100) NOT NULL, ESCALATED_USERID VARCHAR(100) NOT NULL,ESCALATED_USERNAME VARCHAR(100) NOT NULL, ESCALATION_TEXT VARCHAR(1000) NOT NULL, CLIENT VARCHAR(200) NOT NULL, DATE_OF_REPORT DATE NOT NULL,DATE_OF_RESOLUTION DATE, FUNCTIONAL_TEAM VARCHAR(100), IBMISSUE VARCHAR(10) NOT NULL, ESCALATION_RAISED_BY VARCHAR(100) NOT NULL,SEVERITY VARCHAR(10) NOT NULL, TICKETNO VARCHAR(100) ,TICKET_CATEGORY VARCHAR(100), TYPE VARCHAR(20) NOT NULL, USERID VARCHAR(100) NOT NULL,USERNAME VARCHAR(100) NOT NULL,ACTION_TAKEN VARCHAR(1000), PRIMARY KEY(ID))

7)db2 create table report.otherform( ID VARCHAR(100) NOT NULL, APPRECIATED_USERID VARCHAR(100) NOT NULL,APPRECIATED_USERNAME VARCHAR(100) NOT NULL,APPRTXT VARCHAR(1000) NOT NULL, OTHER VARCHAR(100) NOT NULL, TDATE DATE NOT NULL, FUNCTIONAL_TEAM VARCHAR(100) NOT NULL,APPRECIATED_BY VARCHAR(100) NOT NULL ,REASON VARCHAR(100), STATUS VARCHAR(20) NOT NULL, TYPE VARCHAR(100) NOT NULL, USERID VARCHAR(100) NOT NULL,USERNAME VARCHAR(100) NOT NULL , PRIMARY KEY(ID))

  8)db2 create table report.parature(  CID VARCHAR(100) NOT NULL,  PARATURE_NAME VARCHAR(200) NOT NULL, LDAP_NAME VARCHAR(200) NOT NULL,IBM_INTRANET_ID VARCHAR(100) NOT NULL, PRIMARY KEY(CID))


-->run node migrate.js
